{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "aspName": {
           "type": "string",
           "metadata": {
                "description": "app service plan name"
            }
        },
        "webappname": {
           "type": "string",
           "metadata": {
                "description": "webapp name"
            }
        }
    },
    "variables": {
        "aspprefix": "[parameters('aspName')]",
        "webappprefix": "[parameters('webappname')]"
    },
    "resources": [
        {
            "name": "[variables('aspprefix')]",
            "type": "Microsoft.Web/serverfarms",
            "apiVersion": "2018-02-01",
            "location": "[resourceGroup().location]",
            "sku": {
                "name": "S1",
                "capacity": 1
            },
            "properties": {
                "name": "[variables('aspprefix')]"
            }
        },
        {
            "name": "[variables('webappprefix')]",
            "type": "Microsoft.Web/sites",
            "apiVersion": "2016-08-01",
            "location": "[resourceGroup().location]",
            "dependsOn": [
                "[resourceId('Microsoft.Web/serverfarms', variables('aspprefix'))]"
            ],
            "properties": {
                "name": "[variables('webappprefix')]",
                "serverFarmId": "[resourceId('Microsoft.Web/serverfarms', variables('aspprefix'))]"
            }
        }
    ],
    "outputs": {}
}