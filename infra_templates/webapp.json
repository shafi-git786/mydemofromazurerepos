{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "aspplanprefix": {
            "type": "string"
            
        },
        "webappprefix": {
            "type": "string"
        }
    },
    "functions": [],
    "variables": {
        "aspplan": "[concat(parameters('aspplanprefix'),uniqueString(resourceGroup().id))]",
        "webappname": "[concat(parameters('webappprefix'),uniqueString(resourceGroup().id))]"
    },
    "resources": [
        {
            "name": "[variables('aspplan')]",
            "type": "Microsoft.Web/serverfarms",
            "apiVersion": "2018-02-01",
            "location": "[resourceGroup().location]",
            "sku": {
                "name": "S1",
                "capacity": 1
            },
            "tags": {
                "displayName": "[variables('aspplan')]"
            },
            "properties": {
                "name": "[variables('aspplan')]"
            }
        },
        {
            "type": "Microsoft.Web/sites",
            "apiVersion": "2018-11-01",
            "name":"[variables('webappname')]",
           "dependsOn": ["[resourceId('Microsoft.Web/serverfarms', variables('aspplan'))]"],
           "location":"[resourceGroup().location]",
           "properties": {
               "serverFarmId": "[resourceId('Microsoft.Web/serverfarms',variables('aspplan'))]"
           }
        }
    ],
    "outputs": {}
}